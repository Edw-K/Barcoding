<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:TSD.Views.FlyoutPages"
            x:Class="TSD.Views.FlyoutPages.AppPage">
    <FlyoutPage.Resources>
        <ResourceDictionary Source="../Resources/Resources_XML/ResourceDictionaryMobile.xaml"/>
    </FlyoutPage.Resources>
    <FlyoutPage.Flyout>
        <pages:AppPageFlyout x:Name="FlyoutPage"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:UniversalView/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>