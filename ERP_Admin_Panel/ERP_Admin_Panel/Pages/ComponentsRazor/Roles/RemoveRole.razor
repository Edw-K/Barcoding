@inherits RemoveRoleModel

<div class="modal-body">
    <p>Вы действительно хотите удалить роль @(Role == null ? string.Empty : $"\"{Role.Name}\"")?</p>
</div>
<div class="modal-footer justify-content-center">
    <button class="btn btn-danger" @onclick="Cancel">Нет</button>
    <button class="btn btn-primary" @onclick="RemoveRole">Да</button>
</div>