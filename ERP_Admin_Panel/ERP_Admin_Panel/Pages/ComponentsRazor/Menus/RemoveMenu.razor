@inherits RemoveMenuModel

<div class="modal-body">
    <p>Вы действительно хотите удалить меню @(Menu == null ? string.Empty : $"\"{Menu.Name}\"")?</p>
</div>
<div class="modal-footer justify-content-center">
    <button class="btn btn-danger" @onclick="Cancel">Нет</button>
    <button class="btn btn-primary" @onclick="RemoveMenu">Да</button>
</div>